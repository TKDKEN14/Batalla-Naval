<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="326.0" prefWidth="333.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.batallanaval.HelloController">
  <GridPane prefHeight="220.0" prefWidth="166.0">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="169.0" minWidth="10.0" prefWidth="148.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints maxHeight="80.0" minHeight="10.0" prefHeight="47.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="69.0" minHeight="0.0" prefHeight="37.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="95.0" minHeight="10.0" prefHeight="57.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="67.0" minHeight="0.0" prefHeight="67.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>

      <Label fx:id="welcomeText" text="Jugador:">
        <GridPane.margin>
          <Insets right="10.0" top="30.0" />
        </GridPane.margin>
      </Label>
      <Button fx:id="BotonJugar" minHeight="-Infinity" minWidth="-Infinity" onAction="#onJugarButtonClick" prefHeight="40.0" prefWidth="70.0" text="Jugar" GridPane.rowIndex="2">
        <GridPane.margin>
          <Insets left="45.0" />
        </GridPane.margin>
      </Button>
      <TextField fx:id="Usuario" GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets bottom="5.0" />
        </GridPane.margin>
      </TextField>
      <Button fx:id="BotonSalir" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onExitButtonClick" prefHeight="40.0" prefWidth="70.0" text="Salir" GridPane.rowIndex="3">
        <GridPane.margin>
          <Insets left="45.0" />
        </GridPane.margin>
      </Button>
    </children>
    <VBox.margin>
      <Insets bottom="40.0" left="90.0" right="100.0" />
    </VBox.margin>
  </GridPane>
</VBox>
